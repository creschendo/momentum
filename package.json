{
  "name": "momentum",
  "private": true,
  "version": "0.1.0",
  "workspaces": [
    "client",
    "server"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev --prefix server\" \"npm run dev --prefix client\"",
    "start": "node server/index.js",
    "test": "vitest",
    "db:init": "npm --prefix server run db:init",
    "seed:user": "npm --prefix server run seed:user",
    "auth:setup": "npm --prefix server run auth:setup"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "vitest": "^1.3.0"
  }
}
